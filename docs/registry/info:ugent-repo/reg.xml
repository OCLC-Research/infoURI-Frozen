	<info-namespace>ugent-repo</info-namespace>
        <namespace-title>Namespace of identifiers for the University Library Ghent information assets</namespace-title>
	<registration-date>2005-03-22</registration-date>
	<namespace-authority>
		<organisation-name>Ghent University, Central Library</organisation-name>
